<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Haiping Chen</Authors>
    <Company>Personal</Company>
    <Description>.Net implementation of Etherscan APIs</Description>
    <Copyright>MIT</Copyright>
    <PackageLicenseUrl>https://github.com/Haiping-Chen/Etherscan.NetSDK/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Haiping-Chen/Etherscan.NetSDK</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Haiping-Chen/Etherscan.NetSDK</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Etherscan blockchain Ethereum</PackageTags>
    <Version>0.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DotNetToolkit" Version="1.2.0" />
    <PackageReference Include="RestSharp" Version="106.2.1" />
  </ItemGroup>

</Project>
